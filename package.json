{
   "name": "invoiceware",
   "version": "1.0.0",
   "description": "",
   "main": "index.js",
   "scripts": {
      "build": "rimraf dist && tsc -p tsconfig-build.json",
      "lint": "eslint . --fix",
      "test": "jest --passWithNoTests --no-cache --runInBand",
      "test:watch": "npm test -- --watch",
      "test:staged": "npm test -- --findRelatedTests",
      "test:coverage": "npm test -- --coverage",
      "prepare": "husky install",
      "typeorm": "typeorm-ts-node-commonjs",
      "generate": "npm run typeorm migration:generate -- -d ./src/main/config/ormconfig ./src/infra/db/migration/script",
      "create": "npm run typeorm migration:create -- ./src/infra/db/migration/script",
      "run": "npm run typeorm migration:run -- -d ./src/main/config/ormconfig"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "devDependencies": {
      "@types/jest": "^29.5.2",
      "@types/module-alias": "^2.0.1",
      "@types/node": "^20.2.5",
      "@types/pg": "^8.10.2",
      "@typescript-eslint/eslint-plugin": "^5.59.9",
      "eslint": "^8.42.0",
      "eslint-config-standard-with-typescript": "^35.0.0",
      "eslint-plugin-import": "^2.27.5",
      "eslint-plugin-n": "^15.7.0",
      "eslint-plugin-promise": "^6.1.1",
      "git-commit-msg-linter": "^5.0.4",
      "husky": "^8.0.3",
      "jest": "^29.5.0",
      "lint-staged": "^13.2.2",
      "ts-jest": "^29.1.0",
      "typescript": "^5.1.3"
   },
   "engines": {
      "node": "18.16.0"
   },
   "dependencies": {
      "dotenv": "^16.1.4",
      "module-alias": "^2.2.3",
      "pg": "^8.11.0",
      "rimraf": "^5.0.1",
      "typeorm": "^0.3.16"
   }
}